      \strng{namehash}{80e8af0103942543b138e799c9280739}
      \strng{fullhash}{6f08f2a4c9f61767cca2009684c37d19}
      \strng{bibnamehash}{80e8af0103942543b138e799c9280739}
      \strng{authorbibnamehash}{80e8af0103942543b138e799c9280739}
