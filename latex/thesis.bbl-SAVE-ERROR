      \strng{namehash}{178f5a62c7fd1b95f66734e8d968d35b}
      \strng{fullhash}{178f5a62c7fd1b95f66734e8d968d35b}
      \strng{bibnamehash}{178f5a62c7fd1b95f66734e8d968d35b}
      \strng{authorbibnamehash}{178f5a62c7fd1b95f66734e8d968d35b}
      \strng{authornamehash}{178f5a62c7fd1b95f66734e8d968d35b}
