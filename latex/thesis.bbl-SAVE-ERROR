      \strng{authorfullhash}{dbdd9d32a910463df22995547ef298af}
      \strng{editorbibnamehash}{77f6438cd64818c3b87df88aad2e1dac}
      \strng{editornamehash}{77f6438cd64818c3b87df88aad2e1dac}
