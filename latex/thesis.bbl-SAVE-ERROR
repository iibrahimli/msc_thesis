      \strng{namehash}{11cd08f0787836fb5cb14caa2f9b25df}
      \strng{fullhash}{dbdd9d32a910463df22995547ef298af}
      \strng{bibnamehash}{11cd08f0787836fb5cb14caa2f9b25df}
