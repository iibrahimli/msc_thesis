      \strng{namehash}{27dde3b9e4280303e87a9d6d9a23ce4f}
      \strng{fullhash}{c27435797b65dbddbb3e19bcab82a709}
      \strng{bibnamehash}{27dde3b9e4280303e87a9d6d9a23ce4f}
      \strng{authorbibnamehash}{27dde3b9e4280303e87a9d6d9a23ce4f}
      \strng{authornamehash}{27dde3b9e4280303e87a9d6d9a23ce4f}
